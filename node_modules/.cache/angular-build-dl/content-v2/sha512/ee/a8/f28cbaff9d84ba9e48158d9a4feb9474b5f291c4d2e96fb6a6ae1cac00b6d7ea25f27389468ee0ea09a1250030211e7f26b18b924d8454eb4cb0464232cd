let CmdMapPage = class CmdMapPage {
    constructor(navCtrl, platform, geolocation, database, device) {
        this.navCtrl = navCtrl;
        this.platform = platform;
        this.geolocation = geolocation;
        this.database = database;
        this.device = device;
        this.markers = [];
        this.ref = firebase__WEBPACK_IMPORTED_MODULE_6__["database"]().ref('geolocations/');
        platform.ready().then(() => {
            this.initMap();
        });
        this.ref.on('value', resp => {
            this.deleteMarkers();
            snapshotToArray(resp).forEach(data => {
                let image = 'assets/car.png';
                let updatelocation = new google.maps.LatLng(data.latitude, data.longitude);
                this.addMarker(updatelocation, image, this.database.current.user.name);
            });
        });
    }
    ;
    initMap() {
        var c = 0;
        var x = 0;
        var y = 0;
        console.log(this.database.current.user);
        this.geolocation.getCurrentPosition({ maximumAge: 3000, timeout: 2500, enableHighAccuracy: true }).then((resp) => {
            let mylocation = new google.maps.LatLng(resp.coords.latitude, resp.coords.longitude);
            this.database.current.user.location = { lat: resp.coords.latitude, lng: resp.coords.longitude };
            this.database.write('user', this.database.current.user.email, this.database.current.user);
            this.map = new google.maps.Map(this.mapElement.nativeElement, {
                zoom: 15,
                center: mylocation
            });
        });
        for (var driver of this.database.current.all_drivers_of_commander) {
            var drvrloc = new google.maps.LatLng(driver.location.lat, driver.location.lng);
            let image = 'assets/car.png';
            this.addMarker(drvrloc, image, driver.name);
            c += 1;
            x += driver.location.lat;
            y += driver.location.lng;
        }
        var midx = x / c;
        var midy = y / c;
        var midloc = new google.maps.LatLng(midx, midy);
        this.map.panTo(midloc);
    }
    addMarker(location, image, n) {
        let marker = new google.maps.Marker({
            position: location,
            map: this.map,
            icon: image,
            label: n
        });
        this.markers.push(marker);
    }
    deleteMarkers() {
        this.markers = [];
    }
    updateGeolocation(uuid, lat, lng) {
        if (localStorage.getItem('mykey')) {
            firebase__WEBPACK_IMPORTED_MODULE_6__["database"]().ref('geolocations/' + localStorage.getItem('mykey')).set({
                uuid: uuid,
                latitude: lat,
                longitude: lng
            });
        }
        else {
            let newData = this.ref.push();
            newData.set({
                uuid: uuid,
                latitude: lat,
                longitude: lng
            });
            localStorage.setItem('mykey', newData.key);
        }
    }
};
